<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>simulation-setup</name>
  <version>4.0.0</version>
  <description>The simulation-setup package</description>
  <maintainer email="trannamtruong98@gmail.com">NamTruongTran</maintainer>
  <license>BSD</license>

  <!-- ament_cmake as CMake build tool used in ROS2 -->
  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <exec_depend>arena_bringup</exec_depend>

  <!-- Metadata about how the package should be exported to other packages -->
  <export>
    <build_type>ament_python</build_type>
    <!-- <build_type>ament_cmake</build_type> -->
  </export>
</package>